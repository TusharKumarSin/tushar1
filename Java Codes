import java.util.Scanner;

 class Practice_set {
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter the number of Rows:");
       int rows=sc.nextInt();
       for(int i=1;i<=rows;i++){
           for(int j=1;j<=(rows-i);j++){
               System.out.print(" ");
           }
           for(int j=i;j>=1;j--){
               System.out.print(j);
           }
           for(int j=2;j<=i;j++){
               System.out.print(j);

           }
           System.out.println();
       }


    }
}


