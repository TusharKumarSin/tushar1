### For new line code

import java.util.Scanner;

 class Practice_set {

         public static void main(String args[]){
             Scanner scanner = new Scanner(System.in);

             System.out.print("Enter the number of elements: ");
             int n = scanner.nextInt(); // Number of elements in the array
             scanner.nextLine(); // Consume the newline

             String[] dynamicArgs = new String[n]; // Create an array for strings

             System.out.println("Enter the elements:");
             for (int i = 0; i < n; i++) {
                 dynamicArgs[i] = scanner.nextLine(); // Populate the array
             }

             // Display the array
             System.out.println("You entered:");
             for (int i = 0; i < dynamicArgs.length; i++) {
                 System.out.println("dynamicArgs[" + i + "]: " + dynamicArgs[i]);
             }

         }
     }

### Checking the entered number whether it is the power of 2 or not:-

import java.util.Scanner;
import java.lang.Math;
 class Practice_set {
         public static void main(String args[]){
             System.out.println("Enter the number :");
             Scanner sc=new Scanner(System.in);
             int n=sc.nextInt();
             if((n>0&&(n&(n-1))==0)){
                 System.out.println(n+" is the power of 2");
             }else{
                 System.out.println(n+" is not the power of 2");
             }
         }
     }

### Numbers of 1s in the entered number is code is:-

import java.util.Scanner;
import java.lang.Math;
 class Practice_set {
         public static void main(String args[]){
             System.out.println("Enter the number :");
             Scanner sc=new Scanner(System.in);
             int n=sc.nextInt();
             int count=0;
             while(n!=0){
                 count+=(n&1);
                 n=n>>1;
             }
             System.out.println(count);
         }
     }

#### Decimal to binary representation and binary to decimal representation function code :-

import java.util.Scanner;
import java.lang.Math;
 class Practice_set {
     public static void binarep(int num,int n){
         int i=0;
         int ans=0;
         while(n!=0){
             int bit=n&1;
             ans=(bit*((int) Math.pow(10, i)))+ans;
             n=n/2;
             i++;

         }
         System.out.println(num+" bit representation is "+ans);
     }
public static void decimalrep(int binary,int bin){
         int i=0;
         int ans=0;
         while(binary!=0){
             int digit=binary%10;
             if(digit==1){
                 ans=ans+((int)Math.pow(2,i));
             }
             binary=binary/10;
             i++;
         }
    System.out.println("Decimal representation of "+bin+" number is "+ans);
}
         public static void main(String args[]){
             System.out.println("Enter the number :");
             Scanner sc=new Scanner(System.in);
             int num=sc.nextInt();
             int n=num;
             decimalrep( num, n);



         }
     }

#### BUBBLE SORT CODE
import java.util.Scanner;
import java.lang.Math;
 class Practice_set {
     // Bubble sort
         public static void main(String args[]){
             int arr[]={7,8,3,1,2};
             for(int i=0;i<arr.length-1;i++){
                 for(int j=0;j<arr.length-i-1;j++){
                     if(arr[j]>arr[j+1]){
                         int temp=arr[j+1];
                         arr[j+1]=arr[j];
                         arr[j]=temp;
                     }
                 }
             }
             for(int i=0;i<arr.length;i++){
                 System.out.print(arr[i]+" ");
             }


         }
     }


### SELECTION SORTING
import java.util.Scanner;
import java.lang.Math;
 class Practice_set {
     // Selection sort
         public static void main(String args[]){
             int arr[]={7,8,3,1,2};
             for(int i=0;i<arr.length-1;i++){
                 int smallest=i;
                 for(int j=i+1;j<arr.length;j++){
                     if(arr[smallest]>arr[j]){
                         smallest=j;
                     }
                 }
                 int temp=arr[smallest];
                 arr[smallest]=arr[i];
                 arr[i]=temp;
             }
             for(int i=0;i<arr.length;i++){
                 System.out.print(arr[i]+" ");
             }


         }
     }

### INSERTION SORT:
import java.util.Scanner;
import java.lang.Math;
 class Practice_set {
     // INSERTION sort
         public static void main(String args[]){
             int arr[]={7,8,3,1,2};
             for(int i=1;i<arr.length;i++){
                 int current=arr[i];
                 int j=i-1;
                 while(j>=0&&current<arr[j]){
                     arr[j+1]=arr[j];
                     j--;
                 }
                 arr[j+1]=current;
             }
             for(int i=0;i<arr.length;i++){
                 System.out.print(arr[i]+" ");
             }


         }
     }













