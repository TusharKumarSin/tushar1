import java.util.Scanner;

 class Practice_set {
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter the number of Rows:");
       int rows=sc.nextInt();
       for(int i=1;i<=rows;i++){
           for(int j=1;j<=(rows-i);j++){
               System.out.print(" ");
           }
           for(int j=1;j<=i;j++){
               System.out.print(i+" ");
           }
           System.out.println();
       }
       for(int i=rows-1;i>=1;i--){
           for(int j=1;j<=(rows-i);j++){
               System.out.print(" ");
           }
           for(int j=1;j<=i;j++){
               System.out.print(i+" ");
           }
           System.out.println();
       }

    }
}

