import java.util.Scanner;

 class Practice_set {
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter the number of Rows:");
       int rows=sc.nextInt();
       int c=1;
       for(int i=1;i<=rows;i++){
           for(int j=1;j<=i;j++){
              if((i+j)%2==0){
                  System.out.print("1");
              }else{
                  System.out.print("0");
              }

           }
           System.out.println();
       }

    }
}

